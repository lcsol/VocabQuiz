<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.sdpvocabquiz">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SDPVocabQuizActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".quizPortal" />
        <activity android:name=".addQuiz" />
        <activity
            android:name=".addQuizCont"
            android:parentActivityName=".addQuiz" />
        <activity android:name=".removeQuiz" />
        <activity android:name=".displayQuizResult" />
        <activity android:name=".practiceQuiz" />
        <activity android:name=".currentPractice" />
        <activity android:name=".finishQuiz" />
        <activity
            android:name=".ViewQuizzes"
            android:label="@string/title_activity_view_quizzes"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".viewQuizzes" />

        <activity android:name=".quizStatistics"></activity>

        <activity android:name=".register" />
        <activity
            android:name=".viewQuizStats"
            android:label="@string/title_activity_view_quiz_stats"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>